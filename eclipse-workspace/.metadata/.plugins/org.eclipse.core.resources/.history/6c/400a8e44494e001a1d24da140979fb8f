package com.syntax.class02;

public class Aritmetic {
	public static void main(String[] args) {
	
		int num1=200;
		int num2=100;
		
		int add =num1+num2;
		int sub =num1-num2;
		int mult =num1*num2;
		int div =num1/num2;
		
		System.out.println(num1+num2);
		System.out.println(num1-num2);
		System.out.println(num1*num2);
		System.out.println(num1/num2);
		
	}

}
