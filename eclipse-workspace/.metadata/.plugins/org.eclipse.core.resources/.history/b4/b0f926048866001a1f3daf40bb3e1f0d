package com.syntax.class11;

public class StringManupulations {

	public static void main(String[] args) {
	String ali=new String("this is very high");
	String ali2=ali;
	String ali3="this is really high";
	boolean veli;
	veli=ali.equals(ali2);
	System.out.println("value="+veli);
	veli=ali.equals(ali3);
	System.out.println("value="+veli);
	
	}
		
		}
			
	
