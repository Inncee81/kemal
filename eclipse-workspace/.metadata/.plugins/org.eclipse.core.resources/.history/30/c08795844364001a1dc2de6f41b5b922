package com.sysntax.repl;

public class Arrays2dRepl80 {

	public static void main(String[] args) {
		int sum =0;
		int[][] a = {
				{1,1,2}, //sum = 4
				{3,1,2}, //sum = 6
				{3,5,3}, //sum = 11
				{0,1,2}  //sum = 3
			};
			for(int i=0;i<4; i++) {
				for (int j = 0; j < a[i].length; j++) {
					sum=sum+a[i][j];	
				}System.out.println();
				
			}
			
				System.out.println(sum);
			}

	}


